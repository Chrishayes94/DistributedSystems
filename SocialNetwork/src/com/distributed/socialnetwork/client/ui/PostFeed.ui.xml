<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel ui:field="panel">
		<g:HorizontalPanel>
			<g:Image title="" styleName="" url=""
				ui:field="avatarImage" width="64" height="64" />
			<g:VerticalPanel>
				<g:HTML title="" styleName="username" ui:field="userName" />
				<g:HTML title="" styleName="time" ui:field="time" />
			</g:VerticalPanel>
		</g:HorizontalPanel>
		
		<g:VerticalPanel>
			<g:Image styleName="" ui:field="image" />
			<g:HTML styleName="" ui:field="content" />	
		</g:VerticalPanel>
		<g:VerticalPanel>
			<g:Anchor styleName="like" ui:field="likeArea" />
			<g:HTML ui:field="likeNo" ></g:HTML>
		</g:VerticalPanel>
		<g:HTML ui:field="breakLine" />
	</g:HTMLPanel>
</ui:UiBinder> 